/* pages/detailed/detailed.wxss */
.tit{
  width: 90%;
  padding-left: 2.5%;
  margin-left: 2.5%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  color: #606060;
  border-top: 1rpx solid #C5C5C5;
}

.list{
  width: 95%;
  margin-left: 2.5%;
  margin-top: 10rpx;
  background-color: white;
  border-bottom: 4rpx solid #F1F1F1;
  height: 80rpx;
  display: flex;
  flex-direction: row;
}
.list_name{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  color: #1D1D1D;
  margin-left: 30rpx;
}
.list_right{
  width: 70%;
  /* border: 1rpx solid black; */
  height: 80rpx;
  text-align: right;
  margin-left: 50rpx;
}
.list_time{
  font-size: 24rpx;
  height: 40rpx;
  line-height: 40rpx;
  color: #C5C5C5;
}
.list_number{
  height: 40rpx;
  line-height: 20rpx;
  font-size: 34rpx;
}
.not{
  width: 100%;
  text-align: center;
  font-size: 22rpx;
  color: #CFCFCF;
}
page{
  background-color: #F2F2F2;
}